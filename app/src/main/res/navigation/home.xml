<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/home"
    app:startDestination="@id/mainTaskFragment"
    xmlns:tools="http://schemas.android.com/tools">
    <fragment
        android:id="@+id/mainTaskFragment"
        android:name="com.onix.internship.task.MainTaskFragment"
        android:label="MainTaskFragment" >
        <action
            android:id="@+id/action_mainTaskFragment2_to_developFragment2"
            app:destination="@id/developFragment" />
        <action
            android:id="@+id/action_mainTaskFragment_to_mDialogFragment"
            app:destination="@id/mDialogFragment" />
    </fragment>
    <fragment
        android:id="@+id/developFragment"
        android:name="com.onix.internship.task.DevelopFragment"
        android:label="DevelopFragment" />
    <dialog
        android:id="@+id/mDialogFragment"
        android:name="com.onix.internship.task.mDialogFragment"
        android:label="mDialogFragment" />
</navigation>